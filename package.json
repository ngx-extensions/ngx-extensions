{
  "name": "ngx-extensions",
  "description": "Reusable angular elements",
  "version": "0.0.4",
  "scripts": {
    "start": "ng serve",
    "build:demo": "ng build",
    "coveralls": "cat ./coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
    "test:lib": "ng test ngx-extensions --watch=false --code-coverage",
    "lint:lib": "ng lint ngx-extensions",
    "start-release": "sh scripts/start-release.sh",
    "pre-release": "sh scripts/pre-finish-release.sh",
    "finish-release": "sh scripts/finish-release.sh",
    "build:app": "ng build --prod",
    "build:lib": "sh scripts/build-lib.sh",
    "publish:lib": "npm run build:lib && sh scripts/npm/pack-lib.sh && sh scripts/npm/publish-lib.sh",
    "generate:changelog": "conventional-changelog -i CHANGELOG.md -s -p angular"
  },
  "bugs": {
    "url": "https://github.com/ngx-extensions/ngx-extensions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngx-extensions/ngx-extensions.git"
  },
  "keywords": [
    "angular",
    "extensions",
    "reusable"
  ],
  "license": "MIT",
  "author": {
    "email": "jotatoledon@gmail.com",
    "name": "JosÃ© Toledo",
    "url": "https://github.com/jotatoledo"
  },
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5",
    "yarn": ">=1.3.2"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.x",
    "@angular/cdk": "6.x",
    "@angular/common": "6.x",
    "@angular/compiler": "6.x",
    "@angular/core": "6.x",
    "@angular/forms": "6.x",
    "@angular/http": "6.x",
    "@angular/material": "6.x",
    "@angular/platform-browser": "6.x",
    "@angular/platform-browser-dynamic": "6.x",
    "@angular/router": "6.x",
    "core-js": "^2.5.4",
    "rxjs": "^6.3.0",
    "screenfull": "^3.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~6.2.6",
    "@angular/compiler-cli": "6.x",
    "@angular/language-service": "6.x",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/gulp": "3.x",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/screenfull": "file:local-types/screenfull",
    "codelyzer": "~4.3.0",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "husky": "^1.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-marbles": "^0.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^7.3.0",
    "ng-packagr": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "protractor": "~5.4.0",
    "require-dir": "^1.1.0",
    "semver": "^5.6.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
